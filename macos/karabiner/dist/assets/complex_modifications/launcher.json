{
  "title": "Launcher (@eduarbo)",
  "rules": [
    {
      "description": "Launch apps by right option+letters.",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "a",
            "modifiers": {
              "mandatory": [
                "right_option"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open '/Applications/Calendar.app'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "c",
            "modifiers": {
              "mandatory": [
                "right_option"
              ],
              "optional": [
                "caps_lock"
              ]
            }
